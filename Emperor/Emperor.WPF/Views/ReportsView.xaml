<UserControl x:Class="Emperor.WPF.Views.ReportsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Emperor.WPF.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="32"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Label FontSize="14" VerticalAlignment="Center" Margin="8,2">Year</Label>
            <ComboBox Width="80" Height="20" VerticalAlignment="Center" ItemsSource="{Binding AllYears}">
               
            </ComboBox>
        </StackPanel>
        
    </Grid>
</UserControl>
